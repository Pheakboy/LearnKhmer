{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/HTML/index.html"
    },
    {
      "source": "/index",
      "destination": "/HTML/index.html"
    },
    {
      "source": "/home",
      "destination": "/HTML/index.html"
    },
    {
      "source": "/constant",
      "destination": "/HTML/constant.html"
    },
    {
      "source": "/consonant",
      "destination": "/HTML/constant.html"
    },
    {
      "source": "/vowel",
      "destination": "/HTML/vowel.html"
    },
    {
      "source": "/independent",
      "destination": "/HTML/independent.html"
    },
    {
      "source": "/contact",
      "destination": "/HTML/contact.html"
    }
  ],
  "headers": [
    {
      "source": "/CSS/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    },
    {
      "source": "/JS/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    },
    {
      "source": "/Image/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    },
    {
      "source": "/(.*)\\.mp3$",
      "headers": [
        {
          "key": "Content-Type",
          "value": "audio/mpeg"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
